{
  "name": "hookuspocus",
  "version": "0.1.0-beta",
  "description": "Hook up all the functions!",
  "main": "src/index.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "publish": "./node_modules/@pika/pack/dist-node/index.bin.js build && npm run minify",
    "pack": "./node_modules/@pika/pack/dist-node/index.bin.js build && npm run minify",
    "minify": "npm run minify-src && npm run minify-umd && npm run minify-src && npm run minify-web",
    "minify-src": "./node_modules/.bin/babel ./pkg/dist-src/index.js --out-file ./pkg/dist-src/index.min.js -s --presets minify",
    "minify-umd": "./node_modules/.bin/babel ./pkg/dist-umd/index.js --out-file ./pkg/dist-umd/index.min.js -s --presets minify",
    "minify-web": "./node_modules/.bin/babel ./pkg/dist-web/index.js --out-file ./pkg/dist-web/index.min.js -s --presets minify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://https://github.com/michael-klein/hookuspocus.git"
  },
  "keywords": [
    "functional",
    "hooks",
    "react",
    "web-components",
    "webcomponents",
    "preact"
  ],
  "author": "Michael Klein",
  "license": "MIT",
  "bugs": {
    "url": "https://https://github.com/michael-klein/hookuspocus/issues"
  },
  "homepage": "https://https://github.com/michael-klein/hookuspocus#readme",
  "dependencies": {},
  "devDependencies": {
    "@pika/pack": "^0.3.1",
    "@pika/plugin-build-umd": "^0.3.9",
    "@pika/plugin-build-web": "^0.3.9",
    "@pika/plugin-standard-pkg": "^0.3.9",
    "pika-plugin-minify": "^0.1.0"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-standard-pkg",
        {
          "exclude": [
            "__tests__/*",
            "example/*",
            "pica-plugin-minify/*"
          ]
        }
      ],
      [
        "@pika/plugin-build-web"
      ],
      [
        "@pika/plugin-build-umd",
        {
          "name": "hookuspocus"
        }
      ],
      [
        "pika-plugin-minify",
        {
          "test": "hi"
        }
      ]
    ]
  }
}
